{    "name": "1.5x",
    "version": "2.0",
    "manifest_version": 2,
    "description": "Build an Extension which on youtube video played automatically speeds it upto 1.5x!!",
    "icons":{
        "16":"assets/speedometer_16.png",
        "32":"assets/speedometer_32.png"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
    "browser_action":{
        "default_title":"1.5x"
    },
    "permissions":
        ["background","tabs","activeTab","https://*/*","http://*/*"]
  }